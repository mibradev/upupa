<%= page.form(model: work_type) do |form| %>
  <%= form.field do %>
    <%= form.label :name %>
    <%= form.text_field :name %>
  <% end %>

  <%= form.field do %>
    <%= form.label :multiplicand %>
    <%= form.number_field :multiplicand, step: "any", min: 0.0 %>
  <% end %>

  <%= form.submit %>
<% end %>
