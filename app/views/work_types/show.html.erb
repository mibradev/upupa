<%= render "application/header" do %>
  <%= render "application/header/heading2", content: "Show Work Type" %>

  <%= render "application/header/actions" do %>
    <%= render "application/header/actions/edit", url: edit_work_type_path(@work_type) %>
    <%= render "application/header/actions/index", url: work_types_path %>
  <% end %>
<% end %>

<%= render "application/description_list" do %>
  <%= render "application/description_list/group", term: "Name", description: @work_type.name %>
  <%= render "application/description_list/group", term: "Multiplicand", description: @work_type.multiplicand %>
<% end %>
