<% page.heading "Work File" %>

<% page.actions do |actions| %>
  <%= actions.edit edit_work_file_path(@work_file) %>
  <%= actions.destroy @work_file %>
  <%= actions.index work_files_path %>
<% end %>

<%= page.description_list do |description_list| %>
  <%= description_list.group do |group| %>
    <%= group.term "Name" %>
    <%= group.description @work_file.name %>
  <% end %>
<% end %>
