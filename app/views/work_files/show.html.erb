<% page.show(@work_file) do |page| %>
  <% page.heading = "Work File" %>
  <% page.actions.index = work_files_path %>
  <% page.actions.edit = edit_work_file_path(@work_file) %>

  <%= page.description_list do |description_list| %>
    <%= description_list.group "Name", @work_file.name %>
  <% end %>
<% end %>
