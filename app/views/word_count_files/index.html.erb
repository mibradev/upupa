<% page.heading "Word Count Files" %>

<% page.actions do |actions| %>
  <%= actions.new new_word_count_word_count_file_path(@word_count) %>
<% end %>

<% if @word_count_files.load.any? %>
  <%= render "table", word_count_files: @word_count_files %>
<% else %>
  <%= page.empty_collection %>
<% end %>
